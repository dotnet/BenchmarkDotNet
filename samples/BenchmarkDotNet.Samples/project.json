{
  "title": "BenchmarkDotNet.Samples",
  "buildOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "keyFile": "../../strongNameKey.snk",
        "strongName": true
      }
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      },
      "dependencies": {
        "BenchmarkDotNet.Diagnostics.Windows": {
          "target": "project"
        }
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.ComponentModel.EventBasedAsync": "4.0.11"
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": {
      "target": "project"
    },
    "BenchmarkDotNet.TestDriven": {
      "target": "project"
    }
  }
}