<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\build\common.props" />
  <PropertyGroup>
    <AssemblyTitle>BenchmarkDotNet</AssemblyTitle>
    <TargetFrameworks>net46;netcoreapp1.1;</TargetFrameworks>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <AssemblyName>BenchmarkDotNet</AssemblyName>
    <PackageId>BenchmarkDotNet</PackageId>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net46' ">
    <ProjectReference Include="..\BenchmarkDotNet.Toolchains.Roslyn\BenchmarkDotNet.Toolchains.Roslyn.csproj" />
    <ProjectReference Include="..\BenchmarkDotNet.Runtime.Classic\BenchmarkDotNet.Runtime.Classic.csproj" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp1.1' ">
    <ProjectReference Include="..\BenchmarkDotNet.Runtime.Core\BenchmarkDotNet.Runtime.Core.csproj" />
  </ItemGroup>
</Project>