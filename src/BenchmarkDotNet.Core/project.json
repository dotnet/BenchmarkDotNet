{
  "title": "BenchmarkDotNet.Core",
  "version": "0.10.1-develop",
  "authors": [ ".NET Foundation and contributors" ],
  "description": "Powerful .NET library for benchmarking",
  "copyright": ".NET Foundation and contributors",
  "packOptions": {
    "owners": [ ".NET Foundation and contributors" ],
    "licenseUrl": "https://github.com/dotnet/BenchmarkDotNet/blob/master/LICENSE.md",
    "projectUrl": "https://github.com/dotnet/BenchmarkDotNet",
    "iconUrl": "https://raw.githubusercontent.com/dotnet/BenchmarkDotNet/master/docs/guide/logo.png",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/dotnet/BenchmarkDotNet"
    },
    "tags": [ "benchmark benchmarking performance" ]
  },
  "language": "en-US",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "xmlDoc": true,
        "keyFile": "../strongNameKey.snk",
        "strongName": true
      }
    }
  },
  "buildOptions": {
    "embed": [ "Templates/*" ],
    "nowarn": [ "1591" ],
    "xmlDoc": true
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "frameworkAssemblies": {
        "System.Management": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      },
      "dependencies": {
        "System.Threading.Tasks.Extensions": "4.0.0",
        "RestSharp": "105.2.3"
      }
    },
    "netcoreapp1.1": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        },
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "Microsoft.DotNet.InternalAbstractions": "1.0.0",
        "System.Xml.XPath.XmlDocument": "4.0.1",
        "RestSharp.NetCore": "105.2.3"
      }
    },
    "uap10.0": {
      "buildOptions": {
        "define": [ "CORE", "UAP" ]
      },
      "dependencies": {
        "Microsoft.NETCore.UniversalWindowsPlatform": "5.2.2",
        "System.Console": "4.3.0",
        "System.Threading.Tasks.Extensions": "4.3.0",
        "System.Reflection": "4.3.0",
        "System.Threading": "4.3.0",
        "System.Xml.XPath.XmlDocument": "4.3.0",
        "System.Runtime.InteropServices.RuntimeInformation": "4.3.0",
        "Microsoft.DotNet.InternalAbstractions": "1.0.500-preview2-1-003177"
      }
    }
  }
}