<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\build\common.props" />
  <PropertyGroup>
    <Description>Powerful .NET library for benchmarking (Diagnostic Tools for Windows)</Description>
    <AssemblyTitle>BenchmarkDotNet.Diagnostics.Windows</AssemblyTitle>
    <TargetFramework>net46</TargetFramework>
    <NoWarn>1701;1702;1705;1591;3001;3003;3002</NoWarn>
    <AssemblyName>BenchmarkDotNet.Diagnostics.Windows</AssemblyName>
    <PackageId>BenchmarkDotNet.Diagnostics.Windows</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\BenchmarkDotNet.Disassembler.x64\BenchmarkDotNet.Disassembler.x64.csproj" />
    <ProjectReference Include="..\BenchmarkDotNet.Disassembler.x86\BenchmarkDotNet.Disassembler.x86.csproj" />
    <ProjectReference Include="..\BenchmarkDotNet\BenchmarkDotNet.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Diagnostics.Runtime" Version="0.9.170626.1" />
    <PackageReference Include="Microsoft.Diagnostics.Tracing.TraceEvent" Version="1.0.41.0" />
    <PackageReference Include="Mono.Cecil" Version="0.9.6.4" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net46' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Disassemblers\net46\win7-x86\BenchmarkDotNet.Disassembler.x86.exe" />
    <EmbeddedResource Include="Disassemblers\net46\win7-x64\BenchmarkDotNet.Disassembler.x64.exe" />
  </ItemGroup>
</Project>