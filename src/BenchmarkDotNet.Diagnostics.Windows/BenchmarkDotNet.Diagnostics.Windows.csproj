<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\build\common.props" />
  <PropertyGroup>
    <Description>Powerful .NET library for benchmarking (Diagnostic Tools for Windows)</Description>
    <AssemblyTitle>BenchmarkDotNet.Diagnostics.Windows</AssemblyTitle>
    <TargetFramework>netstandard2.0</TargetFramework>
    <NoWarn>1701;1702;1705;1591;3001;3003;3002;3009</NoWarn>
    <AssemblyName>BenchmarkDotNet.Diagnostics.Windows</AssemblyName>
    <PackageId>BenchmarkDotNet.Diagnostics.Windows</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\BenchmarkDotNet\BenchmarkDotNet.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Diagnostics.Tracing.TraceEvent" Version="3.0.2" PrivateAssets="contentfiles;analyzers" />
    <PackageReference Include="System.Management" Version="6.0.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.5.0" PrivateAssets="all" />
    <PackageReference Include="System.Collections.Immutable" Version="6.0.0" PrivateAssets="all" />
    <PackageReference Include="System.Memory" Version="4.5.5" PrivateAssets="all" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.0.0" PrivateAssets="all" />
  </ItemGroup>
</Project>
