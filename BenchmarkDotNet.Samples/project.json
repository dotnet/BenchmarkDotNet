{
  "title": "BenchmarkDotNet.Samples",
  "buildOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "keyFile": "../strongNameKey.snk",
        "strongName": true
      }
    }
  },
  "frameworks": {
    "net40": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "BenchmarkDotNet.Diagnostics.Windows": "1.0.0-*"
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.ComponentModel.EventBasedAsync": "4.0.11"
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": {
      "target": "project",
      "version": "0.9.7-develop"
    }
  }
}